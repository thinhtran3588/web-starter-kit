  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinhtran3588/web-starter-kit.git"
  },
  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build": "yarn build-server && rm -r -f .next && next build",
    "build-serverless": "yarn build-server && rm -r -f .next && NEXT_TARGET=serverless next build",
    "build-server": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "yarn build-server && nodemon --watch ./dist --watch ./static --inspect=0.0.0.0:5858 --nolazy ./dist/server.js",
    "dev-client": "node ./dist/server.js",
    "gen-schema-query": "node ./dist/schema_query.js",
    "job": "ts-node -O '{\"module\": \"commonjs\"}' job.ts",
    "lint": "eslint '{src,__tests__}/**/**.{ts,tsx}' && prettier --check '{src,__{tests,mocks}__}/**/*.{ts,tsx}'",
    "lint-fix": "prettier --write '{src,__{tests,mocks}__}/**/*.{ts,tsx}' && eslint '{src,__tests__}/**/**.{ts,tsx}' --fix",
    "prettier": "prettier --write '{src,__{tests,mocks}__}/**/*.{ts,tsx}'",
    "serverless-deploy": "cd .serverless-deploy && serverless deploy && cd ..",
    "serverless-prepare": "yarn build-serverless && rm -r -f .serverless-deploy && mkdir .serverless-deploy && cp -r ./dist ./.serverless-deploy/dist && cp ./serverless.package.json ./.serverless-deploy/package.json && cp ./serverless.next.config.js ./.serverless-deploy/next.config.js && cp ./yarn.lock ./.serverless-deploy && cp -r ./.next ./.serverless-deploy/.next && rm -r -f .serverless-deploy/.next/cache && mkdir -p ./.serverless-deploy/static  && cp -r ./static/locales ./.serverless-deploy/static && cp ./serverless.yml ./.serverless-deploy && cd .serverless-deploy && yarn --prod && ../bin/node-prune && cd .. ",
    "serverless-update-s3": "aws s3 rm s3://t-web-starter-kit/static --recursive && aws s3 cp ./static s3://t-web-starter-kit/static --recursive && aws s3 cp s3://t-web-starter-kit/static s3://t-web-starter-kit/static --recursive --metadata-directive REPLACE --acl public-read --cache-control max-age=2592000,public",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "jest --detectOpenHandles --verbose",
    "type-check": "tsc --build tsconfig.json",
    "update-travis-config": "zip -r production.zip environments/production && travis encrypt-file --com production.zip",
    "validate": "yarn lint && yarn type-check && yarn build && yarn test",
    "watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --watch"
  },
  "version": "0.1.0"
}
